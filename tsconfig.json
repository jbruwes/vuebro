{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "lib": ["esnext", "dom", "dom.iterable"],
    "baseUrl": ".",
    "types": ["vite/client", "node"]
  },
  "exclude": [
    "./dist",
    "./.quasar",
    "./node_modules",
    "./src-capacitor",
    "./src-cordova",
    "./quasar.config.*.temporary.compiled*"
  ],
  "vueCompilerOptions": {
    "plugins": ["@vue/language-plugin-pug"]
  }
}
