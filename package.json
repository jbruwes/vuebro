{
  "name": "vues3",
  "version": "3.8.2",
  "description": "Vue.S3 is a program for creating websites from dynamically loaded vue.js templates at runtime and storing them in the S3 cloud storage",
  "productName": "Vue.S3",
  "author": "Jerry Bruwes <jbruwes@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,,html,md,json}\" --ignore-path .gitignore",
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron",
    "build:monolit": "vite build monolit --emptyOutDir",
    "build": "bun run build:monolit && quasar build",
    "build:electron": "bun run build:monolit && quasar build -m electron"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "@electron/remote": "^2.1.2",
    "@iconify/vue": "^4.1.2",
    "@quasar/quasar-ui-qiconpicker": "^2.0.7",
    "@smithy/fetch-http-handler": "^3.1.0",
    "@tresjs/cientos": "^3.9.0",
    "@tresjs/core": "^4.0.2",
    "@types/crypto-js": "^4.2.2",
    "@unhead/vue": "^1.9.14",
    "@unocss/core": "^0.61.0",
    "@unocss/extractor-pug": "^0.61.0",
    "@unocss/preset-attributify": "^0.61.0",
    "@unocss/preset-icons": "^0.61.0",
    "@unocss/preset-tagify": "^0.61.0",
    "@unocss/preset-typography": "^0.61.0",
    "@unocss/preset-uno": "^0.61.0",
    "@unocss/preset-web-fonts": "^0.61.0",
    "@unocss/reset": "^0.61.0",
    "@unocss/runtime": "^0.61.0",
    "@unocss/transformer-compile-class": "^0.61.0",
    "@unocss/transformer-directives": "^0.61.0",
    "@unocss/transformer-variant-group": "^0.61.0",
    "@unocss/vite": "^0.61.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vueuse/components": "^10.11.0",
    "@vueuse/core": "^10.11.0",
    "@vueuse/motion": "^2.2.3",
    "ace-builds": "^1.35.0",
    "ajv": "^8.16.0",
    "ajv-keywords": "^5.1.0",
    "crypto-js": "^4.2.0",
    "js-beautify": "^1.15.1",
    "json-schema-to-ts": "^3.1.0",
    "mime": "^4.0.3",
    "quasar": "^2.16.4",
    "to-xml": "^0.1.11",
    "uuid-random": "^1.3.2",
    "vite": "^5.3.1",
    "vue": "^3.4.29",
    "vue-i18n": "^9.13.1",
    "vue-gtag": "^2.0.1",
    "vue-router": "^4.3.3",
    "vue3-ace-editor": "^2.2.4",
    "vue3-sfc-loader": "^0.9.5",
    "yandex-metrika-vue3": "^1.3.1"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@prettier/plugin-pug": "^3.0.0",
    "@quasar/app-vite": "^2.0.0-beta.14",
    "@quasar/extras": "^1.16.11",
    "@quasar/quasar-app-extension-qiconpicker": "^2.0.7",
    "@types/js-beautify": "^1.14.3",
