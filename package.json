{
  "name": "vues3",
  "version": "3.30.0",
  "description": "vueS3 is a program for creating websites from dynamically loaded vue.js templates at runtime and storing them in the S3 cloud storage",
  "productName": "vueS3",
  "author": "Jerry Bruwes <jbruwes@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "postinstall": "quasar prepare",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron",
    "build:monolit": "vite build monolit --emptyOutDir",
    "build": "vite build monolit --emptyOutDir && quasar build",
    "build:electron": "vite build monolit --emptyOutDir && quasar build -m electron"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.701.0",
    "@electron/remote": "^2.1.2",
    "@iconify/vue": "^4.1.2",
    "@quasar/app-vite": "^2.0.0-rc.4",
    "@quasar/quasar-ui-qiconpicker": "^2.0.7",
    "@smithy/fetch-http-handler": "^4.1.1",
    "@types/crypto-js": "^4.2.2",
    "@unhead/vue": "^1.11.13",
    "@unocss/core": "^0.65.0-beta.2",
    "@unocss/extractor-pug": "^0.65.0-beta.2",
    "@unocss/preset-attributify": "^0.65.0-beta.2",
    "@unocss/preset-icons": "^0.65.0-beta.2",
    "@unocss/preset-tagify": "^0.65.0-beta.2",
    "@unocss/preset-typography": "^0.65.0-beta.2",
    "@unocss/preset-uno": "^0.65.0-beta.2",
    "@unocss/preset-web-fonts": "^0.65.0-beta.2",
    "@unocss/reset": "^0.65.0-beta.2",
    "@unocss/runtime": "^0.65.0-beta.2",
    "@unocss/vite": "^0.65.0-beta.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@volar/monaco": "^2.4.10",
    "@vueuse/core": "^12.0.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "crypto-js": "^4.2.0",
    "json-schema-to-ts": "^3.1.1",
    "mime": "^4.0.4",
    "monaco-editor-core": "^0.52.0",
    "quasar": "^2.17.4",
    "to-xml": "^0.1.11",
    "uuid-random": "^1.3.2",
    "vite": "^6.0.1",
    "vite-plugin-static-copy": "^2.2.0",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.5",
    "vue-router": "^4.5.0",
    "vue-sfc-descriptor-to-string": "^3.0.1",
    "vue3-sfc-loader": "^0.9.5",
    "@vues3/monaco-volar-worker": "^0.4.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.0",
    "@prettier/plugin-pug": "^3.2.0",
    "@quasar/extras": "^1.16.13",
    "@quasar/quasar-app-extension-qiconpicker": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vue/language-plugin-pug": "^2.1.10",
    "autoprefixer": "^10.4.20",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-perfectionist": "^4.1.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-vue": "^9.31.0",
    "eslint-plugin-vue-pug": "^0.6.2",
    "prettier": "^3.4.1",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "pug": "^3.0.3",
    "typescript": "~5.5.4",
    "vite-plugin-checker": "^0.8.0",
    "vue-tsc": "^2.1.10"
  }
}
