{
  "name": "vues3",
  "version": "3.32.4",
  "description": "vueS3 is a program for creating websites from dynamically loaded vue.js templates at runtime and storing them in the S3 cloud storage",
  "productName": "vueS3",
  "author": "Jerry Bruwes <jbruwes@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "postinstall": "quasar prepare",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron",
    "build": "quasar build",
    "build:electron": "quasar build -m electron"
  },
