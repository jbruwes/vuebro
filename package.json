{
  "name": "vues3",
  "version": "3.22.0",
  "description": "Vue.S3 is a program for creating websites from dynamically loaded vue.js templates at runtime and storing them in the S3 cloud storage",
  "productName": "Vue.S3",
  "author": "Jerry Bruwes <jbruwes@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,,html,md,json}\" --ignore-path .gitignore",
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron",
    "build:monolit": "vite build monolit --emptyOutDir",
    "build": "vite build monolit --emptyOutDir && quasar build",
    "build:electron": "vite build monolit --emptyOutDir && quasar build -m electron"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.635.0",
    "@iconify/vue": "^4.1.2",
    "@quasar/quasar-ui-qiconpicker": "^2.0.7",
    "@smithy/fetch-http-handler": "^3.2.4",
    "@types/crypto-js": "^4.2.2",
    "@unhead/vue": "^1.10.0",
    "@unocss/core": "^0.62.2",
    "@unocss/preset-icons": "^0.62.2",
    "@unocss/preset-typography": "^0.62.2",
    "@unocss/preset-uno": "^0.62.2",
    "@unocss/preset-web-fonts": "^0.62.2",
    "@unocss/reset": "^0.62.2",
    "@unocss/runtime": "^0.62.2",
    "@unocss/extractor-pug": "^0.62.2",
    "@unocss/vite": "^0.62.2",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vueuse/core": "^11.0.1",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "crypto-js": "^4.2.0",
    "json-schema-to-ts": "^3.1.0",
    "mime": "^4.0.4",
    "monaco-editor": "^0.50.0",
    "quasar": "^2.16.9",
    "to-xml": "^0.1.11",
    "uuid-random": "^1.3.2",
    "vite": "^5.4.2",
    "vite-plugin-static-copy": "^1.0.6",
    "vue": "^3.4.38",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.4.3",
    "vue3-sfc-loader": "^0.9.5"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@prettier/plugin-pug": "^3.0.0",
    "@quasar/app-vite": "^2.0.0-beta.19",
    "@quasar/extras": "^1.16.12",
    "@quasar/quasar-app-extension-qiconpicker": "^2.0.7",
    "@types/js-beautify": "^1.14.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vue/language-plugin-pug": "^2.0.29",
    "autoprefixer": "^10.4.20",
    "electron": "^31.4.0",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-perfectionist": "^3.2.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sonarjs": "^1.0.4",
    "eslint-plugin-tsdoc": "^0.3.0",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-plugin-vue-pug": "^0.6.2",
    "prettier": "^3.3.3",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "vite-plugin-checker": "^0.7.2",
    "pug": "^3.0.3",
    "typescript": "^5.5.4",
    "vue-tsc": "^2.0.29"
  }
}
